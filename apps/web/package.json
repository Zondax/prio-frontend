    "@testing-library/react": "16.3.0",
    "@types/node": "24.1.0",
    "@types/react": "19.1.9",
    "@types/react-date-range": "1.4.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "4.7.0",
    "jsdom": "26.1.0",
    "sort-package-json": "^3.3.0",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  }
