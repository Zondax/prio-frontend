{
  "name": "mono-web",
  "version": "0.1.0",
  "private": true,
  "description": "mono-web",
  "keywords": [
    "zondax",
    "mono-web"
  ],
  "license": "UNLICENSED",
  "author": "Zondax AG <hello@zondax.ch>",
  "scripts": {
    "build": "next build",
    "deps:check": "echo 'nothing to check'",
    "deps:fix": "echo 'nothing to fix'",
    "deps:update": "pnpm exec npm-check-updates -u -t semver && pnpm install && pnpm run deps:fix",
    "dev": "next dev --turbopack",
    "env:init": "../../scripts/init-env.sh web",
    "start": "next start",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test e2e",
    "test:e2e:debug": "playwright test e2e --debug",
    "test:e2e:install": "playwright install",
    "test:e2e:report": "playwright show-report",
    "test:e2e:sharding": "playwright test e2e -x --reporter=list --output=playwright-report --shard=$PLAYWRIGHT_SHARD",
    "test:e2e:ui": "playwright test e2e --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@noble/hashes": "^1.8.0",
    "@stripe/react-stripe-js": "^3.8.1",
    "@stripe/stripe-js": "^7.7.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "0.5.16",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.11",
    "@zondax/auth-core": "workspace:*",
    "@zondax/auth-web": "workspace:*",
    "@zondax/otel-web": "workspace:*",
    "@zondax/ui-web": "workspace:*",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "^3.2.0",
    "es-toolkit": "^1.39.8",
    "import-in-the-middle": "^1.14.2",
    "input-otp": "1.4.2",
    "lucide-react": "0.533.0",
    "mono-state": "workspace:*",
    "mono-ui": "workspace:*",
    "next": "15.4.4",
    "next-themes": "0.4.6",
    "postcss": "8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "require-in-the-middle": "^7.5.2",
    "tailwind-merge": "3.3.1",
