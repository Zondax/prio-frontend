{
  "name": "prio-expo",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo start -c --android",
    "clean": "rm -rf .expo node_modules",
    "deps:check": "pnpm dlx expo install --pnpm --check",
    "deps:fix": "pnpm dlx expo install --pnpm --fix",
    "deps:update": "pnpm exec npm-check-updates -u -t semver && pnpm install && pnpm run deps:fix",
    "dev": "expo start -c",
    "dev:local": "expo start --dev-client",
    "dev:web": "expo start -c --web",
    "doctor": "pnpm dlx expo-doctor",
    "env:init": "op read op://.prio-dev/m2n2vr22pck3mfnna6qzhasis4/notesPlain > .env",
    "postinstall": "pnpm exec tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "ios": "expo start -c --ios",
    "test": "FORCE_COLOR=1 vitest run --coverage --passWithNoTests",
    "test:watch": "vitest --watch",
    "validate": "pnpm run format:check && pnpm run test",
    "web": "expo start -c --web"
  },
