{
  "name": "mono-expo",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo run:android",
    "clean": "rm -rf .expo node_modules",
    "deps:check": "pnpm dlx expo install --pnpm --check",
    "deps:fix": "pnpm dlx expo install --pnpm --fix",
    "deps:update": "pnpm exec npm-check-updates -u -t semver && pnpm install && pnpm run deps:fix",
    "dev": "expo start -c",
    "dev:local": "expo start --dev-client",
    "dev:web": "expo start -c --web",
    "doctor": "pnpm dlx expo-doctor",
    "env:init": "../../scripts/init-env.sh expo",
    "postinstall": "pnpm exec tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "ios": "expo run:ios",
    "test": "FORCE_COLOR=1 vitest run --coverage --passWithNoTests",
    "test:watch": "vitest --watch",
    "validate": "pnpm run format:check && pnpm run test",
    "web": "expo start -c --web"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@expo/config-plugins": "^10.0.3",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native/normalize-colors": "0.80.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/core": "^7.9.2",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/elements": "^2.4.2",
    "@react-navigation/native": "^7.1.6",
    "@rn-primitives/accordion": "^1.1.0",
    "@rn-primitives/alert-dialog": "^1.1.0",
    "@rn-primitives/aspect-ratio": "^1.1.0",
    "@rn-primitives/avatar": "~1.1.0",
    "@rn-primitives/checkbox": "^1.1.0",
    "@rn-primitives/collapsible": "^1.1.0",
    "@rn-primitives/context-menu": "^1.1.0",
    "@rn-primitives/dialog": "^1.1.0",
    "@rn-primitives/dropdown-menu": "^1.1.0",
    "@rn-primitives/hover-card": "^1.1.0",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/menubar": "^1.1.0",
    "@rn-primitives/navigation-menu": "^1.1.0",
    "@rn-primitives/popover": "^1.1.0",
    "@rn-primitives/portal": "~1.2.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/radio-group": "^1.1.0",
    "@rn-primitives/select": "^1.1.0",
    "@rn-primitives/separator": "^1.1.0",
    "@rn-primitives/slot": "~1.2.0",
    "@rn-primitives/switch": "^1.1.0",
    "@rn-primitives/table": "^1.1.0",
    "@rn-primitives/tabs": "^1.1.0",
    "@rn-primitives/toggle": "^1.1.0",
    "@rn-primitives/toggle-group": "^1.1.0",
    "@rn-primitives/tooltip": "~1.2.0",
    "@rn-primitives/types": "~1.1.0",
    "@zondax/auth-core": "workspace:*",
    "@zondax/ui-common": "workspace:*",
    "babel-preset-expo": "^13.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expo": "^53.0.15",
    "expo-application": "~6.1.5",
    "expo-asset": "^11.1.6",
    "expo-auth-session": "~6.2.0",
    "expo-blur": "14.1.5",
    "expo-constants": "17.1.6",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.2",
    "expo-device": "~7.1.4",
    "expo-font": "13.3.2",
    "expo-haptics": "14.1.4",
    "expo-image": "~2.3.2",
    "expo-linking": "~7.1.6",
    "expo-location": "~18.1.6",
    "expo-navigation-bar": "~4.2.6",
    "expo-router": "~5.1.2",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-updates": "~0.28.15",
    "expo-web-browser": "14.2.0",
    "lucide-react-native": "^0.523.0",
    "mono-state": "workspace:*",
    "nativewind": "^4.1.23",
    "react": "19.0.10",
    "react-dom": "^19.0.0",
    "react-native": "0.79.4",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "2.24.0",
    "react-native-helmet-async": "2.0.4",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.3.7",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@types/react": "^19.0.10",
    "@types/react-test-renderer": "19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "react-refresh": "0.17.0",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "mono-state",
          "@zondax/auth-core",
          "@zondax/auth-web",
          "@zondax/ui-common",
          "lucide-react-native"
        ]
      }
    },
    "install": {
      "exclude": [
        "mono-state",
        "@zondax/auth-core",
        "@zondax/auth-web",
        "@zondax/ui-common"
      ]
    }
  }
}
